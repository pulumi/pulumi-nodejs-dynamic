{
  "name": "@pulumi/dynamic-provider",
  "version": "${VERSION}",
  "description": "Dynamic resource provider for nodejs",
  "license": "Apache-2.0",
  "bin": "bin/index.js",
  "keywords": [
    "pulumi",
    "dynamic"
  ],
  "homepage": "https://pulumi.io",
  "repository": "https://github.com/pulumi/pulumi-nodejs-dynamic",
  "scripts": {
    "format": "prettier --list-different --write .",
    "lint": "tslint -c ../tslint.json -p tsconfig.json",
    "test": "jest",
    "tsc": "tsc --incremental"
  },
  "dependencies": {
    "@pulumi/pulumi": "3.34.0-alpha.1653429916"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/mime": "^2.0.0",
    "@types/node": "^17.0.21",
    "babel-jest": "^27.5.1",
    "install-peers-cli": "^2.2.0",
    "jest": "^27.5.1",
    "json-schema-to-typescript": "^10.1.5",
    "pkg": "^5.6.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.13.0",
    "typescript": "^4.6.2"
  },
  "//": "Need to also bump p/p inside aws and docker dependencies because they're constrained to only stable releases.",
  "resolutions": {
    "@pulumi/pulumi": "3.34.0-alpha.1653429916"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "/bin/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bin/"
    ]
  }
}
